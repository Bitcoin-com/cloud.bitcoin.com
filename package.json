{
  "name": "cloud.bitcoin.com",
  "description": "Web GUI for Bitcoin.com's Cloud",
  "version": "0.0.2",
  "author": "Bitcoin.com",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.5.1",
    "mocha": "^5.2.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.0.5",
    "react-syntax-highlighter": "^8.0.1",
    "recompose": "^0.27.1",
    "rehype-react": "^3.0.3",
    "styled-components": "^3.4.2"
  },
  "keywords": [
    "gatsby",
    "bitcoin",
    "bitcoin cash",
    "wormhole",
    "bch"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write '**/*.js'",
    "dev": "nodemon app.js",
    "test": "nyc --reporter=text mocha --require babel-core/register --timeout 5000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:report": "nyc --reporter=html mocha --require babel-core/register --timeout 5000"
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-standard": "^4.0.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.18.1",
    "nyc": "^11.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bitcoin-com/cloud.bitcoin.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
